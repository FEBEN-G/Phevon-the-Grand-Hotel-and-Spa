{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es6", "es2020"],
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": ["src", "prisma"],
  "exclude": ["node_modules"]
}
